@{
    ViewData["Title"] = "Ingreso";
}


<div class="row">
    <div class="col-md-10">    
        <form asp-action="IngresoHora" asp-route-id="@Model.Id" method="post">
            <p>Hola ... ,¿estás seguro de registrar la hora de entrada?</p>
